{
  "name": "@rock-js/provider-s3",
  "version": "0.10.0",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "types": "./dist/src/index.d.ts",
    "default": "./dist/src/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
