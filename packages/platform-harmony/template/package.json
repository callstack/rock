  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "^0.77.1"
  },
  "devDependencies": {
    "@rock-js/platform-harmony": "^0.11.2",
    "@react-native-oh/react-native-harmony": "0.77.18-1",
    "@react-native-oh/react-native-harmony-cli": "0.77.18-1"
  }
}
